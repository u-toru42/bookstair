create_table :users do |t|
  t.string :email, null: false, index: { unique: true }
  t.string :crypted_password
  t.string :salt
  t.string :user_name, presence: true, length: { maximum: 255 }

  t.timestamps null: false
end

create_table "books", id: false do |t|
  t.bigint :isbn, primary_key: true
  t.string :title
  t.string :author
  t.text :sales_date
  t.text :large_image_url
  t.text :item_url
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end

create_table "bookshelves", force: :cascade do |t|
  t.string :title
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  # t.bigint :user_id
  # t.bigint :isbn, references: "books", renamed_from: "isbn_id", null: false
  # t.index ["user_id"], name: "index_bookshelves_on_user_id"
end

create_table "reviews", force: :cascade do |t|
  t.integer :status
  t.integer :comprehension
  t.text :body, null: false
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.bigint "user_id"
  t.bigint "isbn", references: "books", renamed_from: "isbn_id", null: false
  t.index ["user_id"], name: "index_reviews_on_user_id"
end
add_foreign_key "reviews", "books", column: "isbn", primary_key: "isbn"
